{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Pedro Ferrari @thepedroferrari <pedro@pedroferrari.com>",
  "dependencies": {
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "gatsby": "^2.24.47",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-manifest": "^2.4.23",
    "gatsby-plugin-offline": "^3.2.23",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.27",
    "gatsby-plugin-typescript": "^2.4.18",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-transformer-sharp": "^2.5.13",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn type-check && yarn lint && gatsby build",
    "clean": "gatsby clean",
    "develop": "env-cmd -f ./.env.development gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "serve": "gatsby serve",
    "start": "yarn develop",
    "test": "yarn type-check && yarn lint && echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "type-check:watch": "yarn type-check -- --watch",
    "type-check": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "eslintConfig": {
    "ignorePatterns": [
      ".cache",
      "public",
      "graphql-types.ts"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "globals": {
      "__PATH_PREFIX__": true
    },
    "extends": [
      "airbnb-typescript",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "@typescript-eslint/indent": [
        "off"
      ],
      "@typescript-eslint/quotes": [
        "error",
        "double"
      ],
      "arrow-body-style": [
        "off"
      ],
      "comma-dangle": [
        "off"
      ],
      "import/prefer-default-export": [
        "off"
      ],
      "object-curly-newline": [
        "off"
      ],
      "react/jsx-one-expression-per-line": [
        "off"
      ]
    }
  },
  "devDependencies": {
    "env-cmd": "^10.1.0"
  }
}